universe                = docker
docker_image            = docker-arcluster-dev.dr.corp.adobe.com/mgharbi/karima_genetic:v10

# We need this to get 64GB of /dev/shm
environment = "HIGHMEM=1"

should_transfer_files = no
transfer_executable   = false

output                = log/interactive.out
error                 = log/interactive.err
log                   = log/interactive.log

queue 1
